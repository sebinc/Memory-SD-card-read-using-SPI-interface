lcd_data(0x20);
for(char i=0;i<11;i++)
{
temp1=root1.name[i];
if(temp1==0x20)
{
if(j==0)
{
lcd_data('.');
}
j++;

}
else
{
lcd_data(temp1);
}
}
}

